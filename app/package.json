{
  "name": "oba",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' public/css/styles.css -o public/css/styles.prefix.css",
    "compress:css": "node-sass public/css/styles.prefix.css public/css/styles.min.css --output-style compressed",
    "build:css": "npm-run-all prefix:css compress:css"
