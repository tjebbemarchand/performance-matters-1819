    <script>
        if ('serviceWorker' in navigator) {
            window.addEventListener('load', function () {
                navigator.serviceWorker
                    .register('/service-worker.js')
                    .then(function (registration) {
                        registration.update();
                        // console.log('Service Worker: Registered');
                    }).catch(function (error) {
                        console.log(error);
                    });
            });
        }
